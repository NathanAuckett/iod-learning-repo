{
    "swagger": "2.0",
    "info": {
        "version": "1.0.0",
        "title": "My User Project CRUD",
        "description": "My User Project Application API",
        "license": {
            "name": "MIT",
            "url": "https://opensource.org/licenses/MIT"
        }
    },
    "host": "localhost:3000",
    "basePath": "/",
    "tags": [
        {
            "name": "Calculator",
            "description": "API for Calculus in the system"
        }
    ],
    "schemes": [
        "http"
    ],
    "consumes": [
        "application/json"
    ],
    "produces": [
        "application/json"
    ],
    "paths": {
        "/calculator/add": {
            "get": {
                "tags": [
                    "Addition Service"
                ],
                "summary": "Add 2 numbers",
                "parameters": [
                    {
                        "name": "num1",
                        "in": "query",
                        "description": "The First Number"
                    },
                    {
                        "name": "num2",
                        "in": "query",
                        "description": "The Second number"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "This service allows you to add two numbers together"
                    }
                }
            }
        },

        "/calculator/subtract": {
            "get": {
                "tags": [
                    "Subtraction Service"
                ],
                "summary": "Subtract second number from first number",
                "parameters": [
                    {
                        "name": "num1",
                        "in": "query",
                        "description": "The First Number"
                    },
                    {
                        "name": "num2",
                        "in": "query",
                        "description": "The Second number"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "This service allows you to subtract one number from another"
                    }
                }
            }
        },


        "/calculator/multiply": {
            "get": {
                "tags": [
                    "Multiplication Service"
                ],
                "summary": "Multiply two numbers together",
                "parameters": [
                    {
                        "name": "num1",
                        "in": "query",
                        "description": "The First Number"
                    },
                    {
                        "name": "num2",
                        "in": "query",
                        "description": "The Second number"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "This service allows you to multiply two numbers together"
                    }
                }
            }
        },

        "/calculator/divide": {
            "get": {
                "tags": [
                    "Dividing Service"
                ],
                "summary": "Divide one number by another",
                "parameters": [
                    {
                        "name": "num1",
                        "in": "query",
                        "description": "The First Number"
                    },
                    {
                        "name": "num2",
                        "in": "query",
                        "description": "The Second number"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "This service allows you to divide one number by another"
                    }
                }
            }
        },


        "/calculator/calculate": {
            "get": {
                "tags": [
                    "Calculation"
                ],
                "summary": "Perform a calculation based on a string",
                "parameters": [
                    {
                        "name": "exp",
                        "in": "query",
                        "description": "The calculation to perform as a string"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "This service allows you to perform a calculation based on a string"
                    }
                }
            }
        }


    }
}